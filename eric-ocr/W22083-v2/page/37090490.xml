				</TextEquiv>
			</TextLine>
			<TextLine id="line_9874_5" custom="readingOrder {index: 5;}">
				<Coords points="1871,285 1853,276 1187,284 305,281 283,291 283,337 307,344 521,346 1838,339 1868,332 "/>
