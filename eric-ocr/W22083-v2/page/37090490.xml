				</TextEquiv>
			</TextLine>
			<TextLine id="line_9874_4" custom="readingOrder {index: 4;}">
				<Coords points="1877,243 1868,236 1333,241 296,239 279,246 279,289 292,301 483,304 1838,299 1875,292 "/>
