				</TextEquiv>
			</TextLine>
			<TextLine id="line_9874_2" custom="readingOrder {index: 2;}">
				<Coords points="259,180 259,223 274,233 1006,235 1210,240 1743,238 1862,242 1886,235 1886,184 1865,177 265,170 "/>
