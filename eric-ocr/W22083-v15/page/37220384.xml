				</TextEquiv>
			</TextLine>
			<TextLine id="line_9874_4" custom="readingOrder {index: 4;}">
				<Coords points="333,247 336,298 409,305 1944,309 1968,300 1968,252 1929,244 988,247 351,240 "/>
