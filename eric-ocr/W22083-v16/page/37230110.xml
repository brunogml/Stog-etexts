				</TextEquiv>
			</TextLine>
			<TextLine id="line_9874_4" custom="readingOrder {index: 4;}">
				<Coords points="1884,240 1873,233 1845,230 351,235 284,240 282,290 295,298 1867,293 1882,286 "/>
